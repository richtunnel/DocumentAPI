{
  "name": "demographics-api-express",
  "version": "1.0.0",
  "description": "Demographics API with Express, Service Bus, and Queue Processing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "worker": "node dist/azure-functions/workers/demographicsWorker.js",
    "webhook-processor": "node dist/azure-functions/workers/webhookProcessor.js",
    "test": "jest",
    "testDemo": "node demographics-test.js",
    "test:single": "node dist/azure-functions/__test__/demographics-docs-test.js single 100",
    "test:batch": "node demographics-test.js batch 50",
    "test:webhook": "node demographics-test.js webhook 25",
    "test:error": "node demographics-test.js error",
    "test:all": "node demographics-test.js all 150",
    "monitor": "node log-monitor.js watch",
    "report": "node log-monitor.js report",
    "test-webhook": "node log-monitor.js test-webhook",
    "config": "node log-monitor.js config",
    "full-test": "concurrently \"npm run monitor\" \"sleep 5 && npm run test:all\"",
    "install-deps": "npm install @azure/service-bus @azure/identity @azure/monitor-query @azure/storage-blob concurrently"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/functions": "^4.7.2-preview",
    "@azure/identity": "^4.11.1",
    "@azure/monitor-query": "^1.3.3",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.28.0",
    "@azure/storage-queue": "^12.27.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mssql": "^9.1.7",
    "@types/node-cron": "^3.0.11",
    "@types/winston": "^2.4.4",
    "axios": "^1.6.2",
    "azure-function-express": "^1.1.0",
    "azurite": "^3.35.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
