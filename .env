NODE_ENV=development
SERVER_1PORT=3000
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
API_KEY_ENCRYPTION_KEY=

# Azure Service Bus
SERVICE_BUS_CONNECTION_STRING="Endpoint=sb://your-namespace.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key"
DEMOGRAPHICS_QUEUE_NAME=que-to-partners-demographics
WEBHOOK_QUEUE_NAME=webhook-notifications

# Azure Blob Storage
BLOB_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your-account;AccountKey=your-key;EndpointSuffix=core.windows.net
DOCUMENTS_CONTAINER_NAME=demographicspartnerstorage

# Azure MSSQL Database
AzureWebJobsStorage=
FUNCTIONS_WORKER_RUNTIME=
DB_NAME=
DB_USER=
DB_PASSWORD=
DB_PORT=
REDIS_CONNECTION_STRING=
QUEUE_CONNECTION_STRING=

# Azure Function App Info
AZURE_FUNCTION_APP_NAME="your-demographics-function-app"
AZURE_SUBSCRIPTION_ID="your-subscription-id"
AZURE_RESOURCE_GROUP="your-resource-group"

# Log Monitoring
APPLICATION_INSIGHTS_ID="your-application-insights-id"
LOG_ANALYTICS_WORKSPACE_ID="your-log-analytics-workspace-id"

# Webhook Configuration
WEBHOOK_PROCESSING_INTERVAL=*/5 * * * *
WEBHOOK_RETRY_ATTEMPTS=3
WEBHOOK_TIMEOUT_MS=30000
TEST_WEBHOOK_URL="https://webhook.site/your-unique-id"
WEBHOOK_SECRET="your-webhook-secret"

# Queue Configuration
MAX_CONCURRENT_CALLS=5
MAX_AUTO_RENEW_LOCK_DURATION_MS=300000
WEBSITE_MAX_DYNAMIC_APPLICATION_SCALE_OUT=

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com